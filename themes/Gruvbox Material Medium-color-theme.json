{
    "name": "Gruvbox Material Medium",
    "type": "dark",
    "tokenColors": [{
            "name": "Global settings",
            "settings": {
                "background": "#282828",
                "foreground": "#dfbf8e"
            }
        },
        // Syntax{{{
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#a9b665"
            }
        },
        {
            "name": "String Escape",
            "scope": "constant.character.escape, text.html constant.character.entity.named, punctuation.definition.entity.html",
            "settings": {
                "foreground": "#e3a84e"
            }
        },
        {
            "name": "Boolean",
            "scope": "constant.language.boolean",
            "settings": {
                "foreground": "#d3869b"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#d3869b"
            }
        },
        {
            "name": "Identifier",
            "scope": "variable, support.variable, support.class, support.constant, meta.definition.variable entity.name.function",
            "settings": {
                "foreground": "#7daea3"
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword, modifier, variable.language.this, support.type.object, constant.language",
            "settings": {
                "foreground": "#ea6962"
            }
        },
        {
            "name": "Function call",
            "scope": "entity.name.function, support.function",
            "settings": {
                "foreground": "#a9b665"
            }
        },
        {
            "name": "Storage",
            "scope": "storage.type, storage.modifier",
            "settings": {
                "foreground": "#e78a4e"
            }
        },
        {
            "name": "Modules",
            "scope": "support.module, support.node",
            "settings": {
                "foreground": "#7daea3",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Type",
            "scope": "support.type",
            "settings": {
                "foreground": "#e3a84e"
            }
        },
        {
            "name": "Type",
            "scope": "entity.name.type, entity.other.inherited-class",
            "settings": {
                "foreground": "#e3a84e"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#787878",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Class",
            "scope": "entity.name.type.class",
            "settings": {
                "foreground": "#89b482",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Class variable",
            "scope": "variable.object.property, meta.field.declaration entity.name.function",
            "settings": {
                "foreground": "#7daea3"
            }
        },
        {
            "name": "Class method",
            "scope": "meta.definition.method entity.name.function",
            "settings": {
                "foreground": "#a9b665"
            }
        },
        {
            "name": "Function definition",
            "scope": "meta.function entity.name.function",
            "settings": {
                "foreground": "#a9b665"
            }
        },
        {
            "name": "Template expression",
            "scope": "template.expression.begin, template.expression.end, punctuation.definition.template-expression.begin, punctuation.definition.template-expression.end",
            "settings": {
                "foreground": "#ea6962"
            }
        },
        {
            "name": "Reset embedded/template expression colors",
            "scope": "meta.embedded, source.groovy.embedded, meta.template.expression",
            "settings": {
                "foreground": "#dfbf8e"
            }
        },
        // }}}
        // Markdown{{{
        {
            "name": "Markdown heading",
            "scope": "markup.heading",
            "settings": {
                "foreground": "#e78a4e",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown link",
            "scope": "string.other.link.title.markdown, constant.other.reference.link.markdown",
            "settings": {
                "foreground": "#d3869b"
            }
        },
        {
            "name": "Markdown link text, meta.image.inline.markdown, markup.underline.link.image.markdown",
            "scope": "text.html.markdown meta.link.inline, meta.link.reference",
            "settings": {
                "foreground": "#a9b665",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markdown delimiter",
            "scope": "punctuation.definition.string.begin.markdown, punctuation.definition.string.end.markdown, punctuation.definition.italic.markdown, punctuation.definition.quote.begin.markdown, punctuation.definition.metadata.markdown, punctuation.separator.key-value.markdown, punctuation.definition.constant.markdown",
            "settings": {
                "foreground": "#928374"
            }
        },
        {
            "name": "Markdown bold delimiter",
            "scope": "punctuation.definition.bold.markdown",
            "settings": {
                "foreground": "#928374",
                "fontStyle": "regular"
            }
        },
        {
            "name": "Markdown separator delimiter",
            "scope": "meta.separator.markdown",
            "settings": {
                "foreground": "#928374",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown italic",
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown bold",
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown bold italic",
            "scope": "markup.bold markup.italic, markup.italic markup.bold",
            "settings": {
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "Markdown code delimiter",
            "scope": "punctuation.definition.markdown, punctuation.definition.raw.markdown",
            "settings": {
                "foreground": "#89b482"
            }
        },
        {
            "name": "Markdown code type",
            "scope": "fenced_code.block.language",
            "settings": {
                "foreground": "#89b482"
            }
        },
        {
            "name": "Markdown code block",
            "scope": "markup.fenced_code.block.markdown, markup.inline.raw.string.markdown",
            "settings": {
                "foreground": "#a9b665"
            }
        },
        {
            "name": "Markdown list mark",
            "scope": "punctuation.definition.list.begin.markdown",
            "settings": {
                "foreground": "#ea6962"
            }
        },
        // }}}
        // reStructuredText{{{
        {
            "name": "reStructuredText delimiter",
            "scope": "punctuation.definition.field.restructuredtext, punctuation.separator.key-value.restructuredtext, punctuation.definition.directive.restructuredtext, punctuation.definition.constant.restructuredtext, punctuation.definition.italic.restructuredtext, punctuation.definition.table.restructuredtext",
            "settings": {
                "foreground": "#928374"
            }
        },
        {
            "name": "reStructuredText delimiter bold",
            "scope": "punctuation.definition.bold.restructuredtext",
            "settings": {
                "foreground": "#928374",
                "fontStyle": "regular"
            }
        },
        {
            "name": "reStructuredText aqua",
            "scope": "entity.name.tag.restructuredtext, punctuation.definition.link.restructuredtext, punctuation.definition.raw.restructuredtext, punctuation.section.raw.restructuredtext",
            "settings": {
                "foreground": "#89b482"
            }
        },
        {
            "name": "reStructuredText purple",
            "scope": "constant.other.footnote.link.restructuredtext",
            "settings": {
                "foreground": "#d3869b"
            }
        },
        {
            "name": "reStructuredText red",
            "scope": "support.directive.restructuredtext",
            "settings": {
                "foreground": "#ea6962"
            }
        },
        {
            "name": "reStructuredText green",
            "scope": "entity.name.directive.restructuredtext, markup.raw.restructuredtext, markup.raw.inner.restructuredtext",
            "settings": {
                "foreground": "#a9b665"
            }
        },
        // }}}
        // LaTex{{{
        {
            "name": "LaTex red",
            "scope": "support.function.section.latex",
            "settings": {
                "foreground": "#ea6962"
            }
        },
        {
            "name": "LaTex yellow",
            "scope": "keyword.control.equation.align.latex",
            "settings": {
                "foreground": "#e3a84e"
            }
        },
        {
            "name": "LaTex green",
            "scope": "support.function.general.tex, support.class.math.block.environment.latex",
            "settings": {
                "foreground": "#a9b665"
            }
        },
        {
            "name": "LaTex purple",
            "scope": "keyword.control.preamble.latex",
            "settings": {
                "foreground": "#d3869b"
            }
        },
        {
            "name": "LaTex aqua",
            "scope": "support.class.latex",
            "settings": {
                "foreground": "#89b482"
            }
        },
        {
            "name": "LaTex blue",
            "scope": "meta.group.braces.tex",
            "settings": {
                "foreground": "#7daea3"
            }
        },
        {
            "name": "LaTex orange",
            "scope": "support.function.be.latex, keyword.control.equation.newline.latex",
            "settings": {
                "foreground": "#e78a4e"
            }
        },
        // }}}
        // Html/Xml{{{
        {
            "name": "Html orange",
            "scope": "entity.name.tag.html, entity.name.tag.xml, entity.name.tag.localname.xml",
            "settings": {
                "foreground": "#e78a4e"
            }
        },
        {
            "name": "Html aqua",
            "scope": "punctuation.separator.key-value.html",
            "settings": {
                "foreground": "#89b482"
            }
        },
        {
            "name": "Html yellow",
            "scope": "entity.other.attribute-name.html, entity.other.attribute-name.xml",
            "settings": {
                "foreground": "#e3a84e"
            }
        },
        {
            "name": "Html purple",
            "scope": "meta.tag.metadata.script.start.html",
            "settings": {
                "foreground": "#d3869b"
            }
        },
        {
            "name": "Html delimiter",
            "scope": "punctuation.definition.tag.begin.html, punctuation.definition.tag.end.html, punctuation.definition.tag.xml",
            "settings": {
                "foreground": "#928374"
            }
        },
        // }}}
        // CSS{{{
        {
            "name": "CSS white",
            "scope": "punctuation.section.function.begin.bracket.round.css, punctuation.section.function.end.bracket.round.css, punctuation.separator.list.comma.css",
            "settings": {
                "foreground": "#dfbf8e"
            }
        },
        {
            "name": "CSS red",
            "scope": "entity.other.attribute-name.class.css, entity.other.keyframe-offset.percentage.css",
            "settings": {
                "foreground": "#ea6962"
            }
        },
        {
            "name": "CSS orange",
            "scope": "support.constant.property-value.css",
            "settings": {
                "foreground": "#e78a4e"
            }
        },
        {
            "name": "CSS green",
            "scope": "keyword.other.unit.px.css, entity.other.attribute-name.css, meta.property-value.css",
            "settings": {
                "foreground": "#a9b665"
            }
        },
        {
            "name": "CSS aqua",
            "scope": "support.type.property-name.css",
            "settings": {
                "foreground": "#89b482"
            }
        },
        {
            "name": "CSS blue",
            "scope": "entity.other.attribute-name.pseudo-element.css, entity.other.attribute-name.pseudo-class.css",
            "settings": {
                "foreground": "#7daea3"
            }
        },
        {
            "name": "CSS purple",
            "scope": "keyword.control.at-rule.keyframes.css, entity.name.tag.css, entity.other.keyframe-offset.css",
            "settings": {
                "foreground": "#d3869b"
            }
        },
        // }}}
